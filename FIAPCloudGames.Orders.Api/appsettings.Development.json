{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.ApplicationInsights"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft.AspNetCore": "Error",
                "Microsoft": "Error",
                "System": "Error"
            }
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "restrictToMinimumLevel": "Information"
                }
            },
            {
                "Name": "ApplicationInsights",
                "Args": {
                    "restrictedToMinimumLevel": "Information",
                    "telemetryConverter": "Serilog.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights"
                }
            }
        ],
        "Enrich": [ "FromLogContext" ],
        "Properties": {
            "Application": "FIAPCloudGames.Orders.Api.Development"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "Order": "Host=rdp-fgc.corya2mk640h.us-east-1.rds.amazonaws.com;Port=5432;User Id=postgres;Password=postgres;Database=fgc;"
    },
    "GameApi": {
        "BaseAddress": "http://fiapcloudgames-games-api:80/"
    },
    "Jwt": {
        "Secret": "super-duper-secret-value-that-should-be-in-user-secrets",
        "Issuer": "fiap-cloud-games",
        "Audience": "developers",
        "ExpirationInMinutes": 15
    }
}